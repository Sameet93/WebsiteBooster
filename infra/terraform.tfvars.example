# Example terraform.tfvars for infra
# Copy to terraform.tfvars and fill values before running `terraform apply`

aws_region = "eu-west-1"
app_name = "websitebooster"
environment = "production"
domain_name = "example.com"
hosted_zone_id = "ZXXXXXXXXXXXXX"
create_www_redirect = true
instance_type = "t3.small"
min_instances = 1
max_instances = 2
vpc_id = ""
certificate_request = true
sendgrid_api_key = ""

